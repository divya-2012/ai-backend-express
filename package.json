    "@sentry/node": "^7.118.0",
    "@sentry/profiling-node": "^1.3.2",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.5",
    "razorpay": "^2.9.4",
    "redis": "^4.6.10",
    "redis-rate-limiter": "^1.2.0",
    "resend": "^3.5.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
