{
  "name": "kitty-magic",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "npm run prisma:generate && nodemon -L --watch src --exec ts-node src/server.ts",
    "build": "npm run clean && npm run prisma:generate && tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/config/swagger/*.yaml dist/src && copyfiles -u 1 src/api/v1/views/*.ejs dist/src",
    "clean": "rm -rf dist",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "postinstall": "prisma generate",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org edu-engineering-4f --project node-express ./dist && sentry-cli sourcemaps upload --org edu-engineering-4f --project node-express ./dist"
  },
  "author": "Venkatesan",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@prisma/extension-accelerate": "^1.2.1",
